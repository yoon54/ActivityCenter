@model List<Association>
<div class="page-title">
    <h1>Plan an Activity</h1>
    <a href="/Dashboard"><button class="btn btn-primary">Dashboard</button></a>
    <div class="btn-group pull-right">
   
<a href="/logout"><button class="btn btn-danger">Logout</button></a>
</div>
<div class="jumbotron">
<h1>@ViewBag.getOne.Title</h1>
<div class="col">
    <h2>Description</h2>
    <h3>@ViewBag.getOne.Description</h3>
</div>
<div class="col">
    <h3>Participants: @Model.Count</h3>
    
    @foreach (var g in Model)
    {
        <p>@g.Guest.FirstName @g.Guest.LastName</p>
    }
</div>
</div>
